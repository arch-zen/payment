<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<context id="SqlServerTables" targetRuntime="MyBatis3">
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin"></plugin>
		<plugin type="com.ymatou.payment.mybatisgenerator.RenameModelPlugin">
			<property name="prefixes2Remove" value="tb, ymt, pp" />
			<property name="suffix2Append" value="Po" />
		</plugin>

		<plugin
			type="com.ymatou.payment.mybatisgenerator.ForceCreateUpdateTimePlugin">
			<property name="insertTimeColumns" value="CreatedTime,dAddTime" />
			<property name="lastUpdateTimeColumns" value="LastUpdatedTime" />
			<property name="dbCurrentTimeExpr" value="getdate()" />
		</plugin>
		
		<commentGenerator
			type="com.ymatou.payment.mybatisgenerator.DBCommentGenerator">
			<property name="suppressAllComments" value="false" />
		</commentGenerator>


		<jdbcConnection driverClass="net.sourceforge.jtds.jdbc.Driver"
			connectionURL="jdbc:jtds:sqlserver://172.16.100.172:1433/YmtRelease"
			userId="sa_test" password="123456">
			<property name="remarksReporting" value="true" />
		</jdbcConnection>

		<javaModelGenerator targetPackage="com.ymatou.payment.infrastructure.db.model"
			targetProject="..\payment-infrastructure\src\main\java">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="sqlmap.autogenerated"
			targetProject="..\payment-infrastructure\src\main\resources">
		</sqlMapGenerator>


		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.ymatou.payment.infrastructure.db.mapper"
			targetProject="..\payment-infrastructure\src\main\java">
		</javaClientGenerator>
		
		<table tableName="PP_Payment" domainObjectName="Payment">
			<property name="useActualColumnNames" value="true" />
		</table>

		<table tableName="PP_BussinessOrder" domainObjectName="BussinessOrder">
			<property name="useActualColumnNames" value="true" />
		</table>
		<table tableName="RefundRequest" domainObjectName="RefundRequest">
			<property name="useActualColumnNames" value="true" />
			<ignoreColumn column="DataVersion" />
		</table>

		<table tableName="PP_CompensateProcessInfo" domainObjectName="CompensateProcessInfo">
			<property name="useActualColumnNames" value="true" />
			<ignoreColumn column="DataVersion" />
		</table>

		<table tableName="RefundMiscRequestLog" domainObjectName="RefundMiscRequestLog">
			<property name="useActualColumnNames" value="true" />
		</table>

		<table tableName="Ymt_AlipayNotifyLog" domainObjectName="AlipayNotifyLog">
			<property name="useActualColumnNames" value="true" />
		</table>
		
		<table tableName="PP_AccoutingLog" domainObjectName="AccountingLog">
			<property name="useActualColumnNames" value="true"/>
		</table>
		
		<table tableName="PP_PaymentParam" domainObjectName="PaymentParam">
			<property name="useActualColumnNames" value="true"/>
		</table>
		
		<table tableName="PP_CmbPublicKey" domainObjectName="CmbPublicKey">
			<property name="useActualColumnNames" value="true"/>
		</table>
		
		<table tableName="PP_CmbAggrement" domainObjectName="CmbAggrement">
			<property name="useActualColumnNames" value="true"/>
		</table>
	</context>

</generatorConfiguration>
